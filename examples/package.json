{
  "name": "@thumbmarkjs/examples",
  "version": "1.0.0", 
  "private": true,
  "description": "Example applications for ThumbmarkJS integrations",
  "workspaces": [
    "react-app",
    "vue-app"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
